@ARTICLE{Morris-1997,
  author = {Joseph P. {Morris} and Patrick J. {Fox} and Yi {Zhu}},
  title = {Modeling Low Reynolds Number Incompressible Flows Using {SPH}},
  journal = {Journal of Computational Physics},
  year = {1997},
  volume = {136},
  pages = {214 - 226},
  number = {1},
  abstract = {The method of smoothed particle hydrodynamics (SPH) is extended to
	model incompressible flows of low Reynolds number. For such flows,
	modification of the standard \{SPH\} formalism is required to minimize
	errors associated with the use of a quasi-incompressible equation
	of state. Treatment of viscosity, state equation, kernel interpolation,
	and boundary conditions are described. Simulations using the method
	show close agreement with series solutions for Couette and Poiseuille
